{
  "Name": "Shell Executor",
  "Version": "1.0.0",
  "Author": "LMG",
  "Description": "Execute shell commands from your Stream Deck",
  "Category": "Development",
  "CategoryIcon": "imgs/category",
  "Icon": "imgs/plugin",
  "CodePath": "bin/plugin.js",
  "UUID": "com.lmg.shell-executor",
  "SDKVersion": 2,
  "Software": {
    "MinimumVersion": "6.9"
  },
  "OS": [
    {
      "Platform": "mac",
      "MinimumVersion": "10.15"
    }
  ],
  "Nodejs": {
    "Version": "20",
    "Debug": "enabled"
  },
  "Actions": [
    {
      "Name": "Execute Command",
      "UUID": "com.lmg.shell-executor.execute",
      "Icon": "imgs/action",
      "Tooltip": "Execute a shell command",
      "PropertyInspectorPath": "ui/property-inspector.html",
      "States": [
        {
          "Image": "imgs/action"
        }
      ],
      "Controllers": ["Keypad"],
      "Encoder": {
        "layout": "$X1"
      }
    }
  ]
}
